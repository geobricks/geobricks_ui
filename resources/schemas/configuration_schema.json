{
   "$schema": "http://json-schema.org/draft-04/schema#",
   "type": "object",
   "title": "FAOSTAT",
   "description": "FAOSTAT",
   "definitions": {
      "lang": {
         "type": "string",
         "default": "en",
         "enum": ["en", "fr", "es", "ar", "cn", "ru"]
      }
   },
   "properties": {
      "download": {
         "type": "object",
         "title": "Geobricks Download",
         "description": "Data acquisition from publicly available datasources.",
         "properties": {
            "url_datasources": {
               "type": "string",
               "format": "url",
               "default": "http://localhost:5555/discovery/datasource/"
            }
         }
      }
   }
}